{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Food Safety Certificate",
    "type": "object",
    "properties": {
      "certificate_type": {
        "type": "string"
      },
      "expiry_date": {
        "type": "string",
        "format": "date"
      },
      "issuer": {
        "type": "string"
      }
    },
    "required": ["certificate_type", "expiry_date", "issuer"]
}
  